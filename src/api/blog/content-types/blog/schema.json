{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Title": {
      "type": "string"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Techniques",
        "Etiquette",
        "Travel",
        "Equipment",
        "Beginners",
        "History"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "Title"
    },
    "Description": {
      "type": "blocks"
    },
    "Excerpt": {
      "type": "string"
    },
    "Tags": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    }
  }
}
